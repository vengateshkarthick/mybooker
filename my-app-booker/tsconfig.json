{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@shared/*": ["./app/shared/*"],
      "@components/*" :["./app/components/*"],
      "@assests/*": ["./app/assests/*"],
      "@api/*": ["./app/api/*"],
      "@actions/*": ["./app/actions/*"],
      "@brooker-next-auth": ["./app/api/auth/[...nextauth].ts"],
      "@brooker-prismadb": ["./app/libs/primsdb.ts"],
      "@booker-types/*": ["./app/types/index.ts"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
